# Copy this file to secrets.tfvars and fill in your actual values
# secrets.tfvars is gitignored and should NEVER be committed

# Fly.io API Token
fly_api_token = "your-fly-api-token-here"

# Neon API Key
neon_api_key = "your-neon-api-key-here"

# GitHub Token
github_token = "your-github-token-here"

# AWS Credentials (for Cognito)
# Note: Terraform AWS provider uses environment variables or AWS credentials file
# Set these as environment variables before running Terraform:
# $env:AWS_ACCESS_KEY_ID="..."
# $env:AWS_SECRET_ACCESS_KEY="..."
aws_access_key_id = "your-aws-access-key-id"
aws_secret_access_key = "your-aws-secret-access-key"

